<!DOCTYPE html>
<html>
<head>
  <title>AsciiCastsDemo</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<h1>RailsCasts</h1>

	<% flash.each do |key, msg| %>
		<%= content_tag :p, msg, :id => key %>
	<% end %>

	<%= yield %>
</body>
</html>
